&adsp_mem {
	size = <0 0x10000000>;
};

&msm_cpufreq {
	qcom,cpufreq-table-0 = <1593600>;
	qcom,cpufreq-table-2 = <1670400>;
};

&soc {
	qcom,cnss {
		/delete-property/ qcom,wlan-ramdump-dynamic;
	};
};

&usb3 {
	/delete-property/ vbus_dwc3-supply;
};
